<Application x:Class="FangJia.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="/Views/MainWindow.xaml"
             Startup="App_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/SkinColors.xaml" />
                <ResourceDictionary Source="Resources/Fonts.xaml" />
                <ResourceDictionary Source="Resources/buttons.xaml"/>
                <ResourceDictionary Source="Resources/Grids.xaml"/>
                <ResourceDictionary Source="Resources/TextBlocks.xaml"/>
                <ResourceDictionary Source="Resources/ListBoxs.xaml"/>
                <ResourceDictionary Source="Resources/GroupBoxs.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
